  --name pgadmin_devfriend \
  -p 8880:80 \
  -e PGADMIN_DEFAULT_EMAIL=admin@example.com \
  -e PGADMIN_DEFAULT_PASSWORD=supersecret \
  -v pgadmin_data_devfriend:/var/lib/pgadmin \
  dpage/pgadmin4
